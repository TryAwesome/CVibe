# AI Engine Environment Variables
# Copy to .env and fill in your values

# gRPC Server
AI_ENGINE_GRPC_PORT=50051
AI_ENGINE_GRPC_MAX_WORKERS=10

# Redis (optional - for session persistence)
AI_ENGINE_REDIS_URL=redis://localhost:6379

# Default LLM Configuration (used when user hasn't configured their own)
# Leave empty to require users to configure their own
OPENAI_API_KEY=
AI_LLM_PROVIDER=openai
AI_LLM_MODEL=gpt-4o
AI_LLM_BASE_URL=

# Vision Model (for resume image parsing)
# If not set, will use same as LLM config
AI_VISION_API_KEY=
AI_VISION_PROVIDER=openai
AI_VISION_MODEL=gpt-4o
AI_VISION_BASE_URL=
